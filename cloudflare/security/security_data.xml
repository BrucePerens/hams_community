<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <record id="group_cloudflare_service" model="res.groups">
            <field name="name">Cloudflare Edge Service Account</field>
        </record>
        <record id="user_cloudflare_service" model="res.users">
            <field name="name">Cloudflare Queue Service</field>
            <field name="login">cloudflare_service_internal</field>
            <field name="is_service_account" eval="True"/>
            <field name="active" eval="True"/>
            <field name="group_ids" eval="[(6, 0, [ref('ham_cloudflare.group_cloudflare_service')])]"/>
        </record>
    </data>
</odoo>
